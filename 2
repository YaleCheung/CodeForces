#include <iostream>
#include <string>
using namespace std;

int main(int argc, char* argv[]) {
    string str;
    cin >> str;
    auto size = str.size();
    string ret;
    int pre = -1;
    for(auto i = 0; i < size;) {
        if ("UWB" == str.substr(i, 3))
            i += 3;
        else {
            if (i != 0)
                ret += " ";
        }
    }
    cout << ret << endl;
    return 0;
}
